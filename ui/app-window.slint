export component AppWindow inherits Window {
    width: 1280px;
    height: 720px;
    // https://github.com/slint-ui/slint/discussions/1229
    out property window_width <=> self.width;
    out property window-height <=> self.height;
    in property video-frame <=> image.source;

    image := Image {
        width: 100%;
        height: 100%;
    }
}
